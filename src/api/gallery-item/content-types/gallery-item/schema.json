{
  "kind": "collectionType",
  "collectionName": "gallery_items",
  "info": {
    "singularName": "gallery-item",
    "pluralName": "gallery-items",
    "displayName": "Gallery Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "prompt": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Architecture",
        "Concept Art",
        "Texture",
        "Reference",
        "Photorealistic"
      ]
    },
    "aspectRatio": {
      "type": "string"
    },
    "tags": {
      "type": "json"
    }
  }
}
